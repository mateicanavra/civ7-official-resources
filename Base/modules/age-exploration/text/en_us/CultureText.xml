<?xml version="1.0" encoding="utf-8"?>
<Database>
	<EnglishText>
		<!-- ==================================================	-->
		<!--	MAIN TREE							-->
		<!-- ==================================================	-->
		<Row Tag="LOC_CIVIC_ECONOMICS_NAME">
			<Text>Economics</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ECONOMICS_QUOTE">
			<Text>"Wealth circulates like blood, all parts partake."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_ECONOMICS_QUOTE_AUTHOR">
			<Text>William Penn</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PIETY_NAME">
			<Text>Piety</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PIETY_QUOTE">
			<Text>"I take refuge in the Buddha. I take refuge in the Dharma. I take refuge in the Sangha."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_PIETY_QUOTE_AUTHOR">
			<Text>Ratanattaya Chant</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MERCANTILISM_NAME">
			<Text>Mercantilism</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MERCANTILISM_QUOTE">
			<Text>"Plenty of money maketh generally things dear, and scarcity of money maketh likewise generally things good cheape."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_MERCANTILISM_QUOTE_AUTHOR">
			<Text>Gerard de Malynes</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AUTHORITY_NAME">
			<Text>Authority</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AUTHORITY_QUOTE">
			<Text>"I have collected all the writings and burnt those which were of no use."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_AUTHORITY_QUOTE_AUTHOR">
			<Text>Qin Shi Huang</Text>
		</Row>
		<Row Tag="LOC_CIVIC_INSPIRATION_NAME">
			<Text>Inspiration</Text>
		</Row>
		<Row Tag="LOC_CIVIC_INSPIRATION_QUOTE">
			<Text>"O for a muse of fire."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_INSPIRATION_QUOTE_AUTHOR">
			<Text>William Shakespeare</Text>
		</Row>
		<Row Tag="LOC_CIVIC_COLONIALISM_NAME">
			<Text>Colonialism</Text>
		</Row>
		<Row Tag="LOC_CIVIC_COLONIALISM_QUOTE">
			<Text>"Colonialism is not satisfied merely with holding a people in its grip, but it turns to the past of the oppressed people, and destroys it."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_COLONIALISM_QUOTE_AUTHOR">
			<Text>Frantz Fanon</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BUREAUCRACY_NAME">
			<Text>Bureaucracy</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BUREAUCRACY_QUOTE">
			<Text>"A bureaucracy always tends to become a pedantocracy."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_BUREAUCRACY_QUOTE_AUTHOR">
			<Text>John Stuart Mill</Text>
		</Row>
		<Row Tag="LOC_CIVIC_DIPLOMATIC_SERVICE_NAME">
			<Text>Diplomatic Service</Text>
		</Row>
		<Row Tag="LOC_CIVIC_DIPLOMATIC_SERVICE_QUOTE">
			<Text>"Since words are not fine, fine words are not sincere."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_DIPLOMATIC_SERVICE_QUOTE_AUTHOR">
			<Text>Lao Tzu</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOCIETY_NAME">
			<Text>Society</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOCIETY_QUOTE">
			<Text>"The social pact ensures that men become equal by covenant and by right."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOCIETY_QUOTE_AUTHOR">
			<Text>Jean-Jacques Rousseau</Text>
		</Row>
		<Row Tag="LOC_CIVIC_IMPERIALISM_NAME">
			<Text>Imperialism</Text>
		</Row>
		<Row Tag="LOC_CIVIC_IMPERIALISM_QUOTE">
			<Text>"Every empire tells itself that its mission is not to plunder and control but to educate and liberate."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_IMPERIALISM_QUOTE_AUTHOR">
			<Text>Edward Said</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOVEREIGNTY_NAME">
			<Text>Sovereignty</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOVEREIGNTY_QUOTE">
			<Text>"The spear and the title go together."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOVEREIGNTY_QUOTE_AUTHOR">
			<Text>Nuer Saying</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOCIAL_CLASS_NAME">
			<Text>Social Class</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOCIAL_CLASS_QUOTE">
			<Text>"When monks are righteous, rajahs are righteous. When ministers are righteous, brahmins and householders are righteous. Thus townsfolk and villagers are righteous."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_SOCIAL_CLASS_QUOTE_AUTHOR">
			<Text>The Book of Gradual Sayings</Text>
		</Row>
		<!-- ==================================================	-->
		<!--	BRANCH TREES							-->
		<!-- ==================================================	-->
		<!-- Branch Tree Reveal -->
		<Row Tag="LOC_REQ_THEOLOGY_HAS_HOLY_CITY_REVEAL_STATUS">
			<Text>Have a Holy City</Text>
		</Row>
		<!-- Branch Tree Nodes -->
		<Row Tag="LOC_CIVIC_THEOLOGY_NAME">
			<Text>Theology</Text>
		</Row>
		<Row Tag="LOC_CIVIC_THEOLOGY_QUOTE">
			<Text>"By knowing things in their true nature, one knows the divine."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_THEOLOGY_QUOTE_AUTHOR">
			<Text>The Book of Causes</Text>
		</Row>
		<Row Tag="LOC_CIVIC_REFORMATION_NAME">
			<Text>Reformation</Text>
		</Row>
		<Row Tag="LOC_CIVIC_REFORMATION_QUOTE">
			<Text>"The gospel cannot be truly preached without offense and tumult."</Text>
		</Row>
		<Row Tag="LOC_CIVIC_REFORMATION_QUOTE_AUTHOR">
			<Text>Martin Luther</Text>
		</Row>
		<Row Tag="LOC_CIVIC_FUTURE_CIVIC_EX_NAME">
			<Text>Future Civic</Text>
		</Row>
		<!-- ==================================================	-->
		<!--	MODIFIER STRINGS							-->
		<!-- ==================================================	-->
		<Row Tag="LOC_MOD_EXPLORATION_TRADITION_SLOT_DESCRIPTION">
			<Text>+1 [icon:NAR_REW_TRADITION_SLOT] Social Policy slot.</Text>
		</Row>
		<Row Tag="LOC_MOD_REFORMATION_BELIEF_DESCRIPTION">
			<Text>Receive 1 additional [icon:NAR_REW_RELIGION] Religious Belief.</Text>
		</Row>
		<Row Tag="LOC_MOD_EX_FUTURE_CIVIC_ATTRIBUTE_EX_DESCRIPTION">
			<Text>+1 [icon:ATTRIBUTE] Random Attribute Point.</Text>
		</Row>
		<Row Tag="LOC_MOD_EX_FUTURE_CIVIC_BOOST_EX_DESCRIPTION">
			<Text>A boost for a non-Civilization unique starting [icon:RADIAL_CIVICS] Civic in the next Age.</Text>
		</Row>
		<Row Tag="LOC_MOD_EXP_FUTURE_CIVIC_AGE_PROGRESS_DESCRIPTION">
			<Text>+10 [icon:NOTIFICATION_TEAM_VICTORIOUS] Age Progress.</Text>
		</Row>
		<!-- ==================================================	-->
		<!--	TRADITIONS							-->
		<!-- ==================================================	-->
		<Row Tag="LOC_TRADITION_BOURGEOISIE_NAME">
			<Text>Bourgeoisie</Text>
		</Row>
		<Row Tag="LOC_TRADITION_BOURGEOISIE_DESCRIPTION">
			<Text>+4 [icon:YIELD_CULTURE] Culture and [icon:YIELD_GOLD] Gold in [icon:YIELD_CITIES] Cities in your Homelands.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CHARTERS_NAME">
			<Text>Charters</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CHARTERS_DESCRIPTION">
			<Text>+1 [icon:YIELD_GOLD] Gold Adjacency for [icon:YIELD_GOLD] Gold Buildings.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CHIVALRY_NAME">
			<Text>Chivalry</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CHIVALRY_DESCRIPTION">
			<Text>+30%[icon:YIELD_PRODUCTION] Production towards training Cavalry Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_COLONIAL_SURPLUS_NAME">
			<Text>Colonial Surplus</Text>
		</Row>
		<Row Tag="LOC_TRADITION_COLONIAL_SURPLUS_DESCRIPTION">
			<Text>+1 [icon:YIELD_PRODUCTION] Production Adjacency for [icon:YIELD_PRODUCTION] Production Buildings.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_COMMISSIONED_OFFICERS_NAME">
			<Text>Commissioned Officers</Text>
		</Row>
		<Row Tag="LOC_TRADITION_COMMISSIONED_OFFICERS_DESCRIPTION">
			<Text>+30% Commander Experience. +1 [icon:Action_Move] Movement for Fleets and Armies.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_COMMUNE_NAME">
			<Text>Commune</Text>
		</Row>
		<Row Tag="LOC_TRADITION_COMMUNE_DESCRIPTION">
			<Text>+20% [icon:YIELD_PRODUCTION] Production towards Overbuilding. +3 [icon:NAR_REW_COMBAT] Combat Strength for [icon:CITY_FORTIFIED] Fortified Districts.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CONSTITUTION_NAME">
			<Text>Constitution</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CONSTITUTION_DESCRIPTION">
			<Text>+25% [icon:YIELD_FOOD] Food and [icon:YIELD_HAPPINESS] Happiness towards maintaining [icon:SPECIALIST] Specialists.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DE_FACTO_NAME">
			<Text>De Facto</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DE_FACTO_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for all Units in Distant Lands. Units gain +5 [icon:Action_Heal] Healing.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DE_JURE_NAME">
			<Text>De Jure</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DE_JURE_PREVIEW">
			<Text>+{1_Amount} De Jure</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DE_JURE_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for all Units in Homelands.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DIVINE_RIGHT_NAME">
			<Text>Divine Right</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DIVINE_RIGHT_DESCRIPTION">
			<Text>+10[icon:YIELD_HAPPINESS] Happiness and +4[icon:YIELD_DIPLOMACY] Influence on the Palace.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_ENLIGHTENMENT_NAME">
			<Text>Enlightenment</Text>
		</Row>
		<Row Tag="LOC_TRADITION_ENLIGHTENMENT_DESCRIPTION">
			<Text>+1 [icon:YIELD_SCIENCE] Science Adjacency for [icon:YIELD_SCIENCE] Science Buildings.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_EVANGELISM_NAME">
			<Text>Evangelism</Text>
		</Row>
		<Row Tag="LOC_TRADITION_EVANGELISM_DESCRIPTION">
			<Text>+1 [icon:Action_Move] Movement for Civilian Units. +1 charge for [icon:UNIT_MISSIONARY] Missionaries.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_HEQIN_NAME">
			<Text>Heqin</Text>
		</Row>
		<Row Tag="LOC_TRADITION_HEQIN_DESCRIPTION">
			<Text>+5 [icon:YIELD_CULTURE] Culture per [icon:PLAYER_RELATIONSHIP_ALLIANCE] Alliance.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_INDENTURE_NAME">
			<Text>Indenture</Text>
		</Row>
		<Row Tag="LOC_TRADITION_INDENTURE_DESCRIPTION">
			<Text>+1 [icon:YIELD_FOOD] Food Adjacency for [icon:YIELD_FOOD] Food Buildings.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_LEVIES_NAME">
			<Text>Levies</Text>
		</Row>
		<Row Tag="LOC_TRADITION_LEVIES_DESCRIPTION">
			<Text>+25% [icon:YIELD_GOLD] Gold towards purchasing Military Units. -1 [icon:YIELD_GOLD] Gold Maintenance for Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_MARITIME_LAW_NAME">
			<Text>Maritime Law</Text>
		</Row>
		<Row Tag="LOC_TRADITION_MARITIME_LAW_DESCRIPTION">
			<Text>+30%[icon:YIELD_PRODUCTION] Production towards training Naval Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_METROPOLE_NAME">
			<Text>Metropole</Text>
		</Row>
		<Row Tag="LOC_TRADITION_METROPOLE_DESCRIPTION">
			<Text>+1 [icon:RADIAL_RESOURCES] Resource Capacity in all [icon:YIELD_CITIES] Cities in Homelands. +10 Range for all [icon:TRADE_ROUTE] Trade Routes.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_PATRONAGE_NAME">
			<Text>Patronage</Text>
		</Row>
		<Row Tag="LOC_TRADITION_PATRONAGE_DESCRIPTION">
			<Text>+1 [icon:YIELD_CULTURE] Culture Adjacency for [icon:YIELD_CULTURE] Culture Buildings.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RATIONALISM_NAME">
			<Text>Rationalism</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RATIONALISM_DESCRIPTION">
			<Text>+15% [icon:YIELD_GOLD] Gold and [icon:YIELD_SCIENCE] Science in your own [icon:YIELD_CITIES] Cities that are converted to your [icon:NAR_REW_RELIGION] Religion.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_REGULARS_NAME">
			<Text>Regulars</Text>
		</Row>
		<Row Tag="LOC_TRADITION_REGULARS_DESCRIPTION">
			<Text>+30%[icon:YIELD_PRODUCTION] Production towards training Infantry and Ranged Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RELIGIOUS_ORDERS_NAME">
			<Text>Religious Orders</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RELIGIOUS_ORDERS_DESCRIPTION">
			<Text>+15% [icon:YIELD_CULTURE] Culture and [icon:YIELD_HAPPINESS] Happiness in your own [icon:YIELD_CITIES] Cities that are converted to your [icon:NAR_REW_RELIGION] Religion.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RENAISSANCE_NAME">
			<Text>Renaissance</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RENAISSANCE_DESCRIPTION">
			<Text>+10% [icon:YIELD_PRODUCTION] Production towards constructing [icon:WONDER] Wonders. +2 [icon:YIELD_CULTURE] Culture on displayed [icon:NAR_REW_GREATWORK] Great Works.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_TARIFFS_NAME">
			<Text>Tariffs</Text>
		</Row>
		<Row Tag="LOC_TRADITION_TARIFFS_DESCRIPTION">
			<Text>+50% [icon:TRADE_INCOME] Trade Income, but -3 [icon:YIELD_HAPPINESS] Happiness in [icon:YIELD_CITIES] Cities.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_TRADE_WINDS_NAME">
			<Text>Trade Winds</Text>
		</Row>
		<Row Tag="LOC_TRADITION_TRADE_WINDS_DESCRIPTION">
			<Text>+1 [icon:YIELD_GOLD] Gold and [icon:YIELD_HAPPINESS] Happiness for every imported [icon:RADIAL_RESOURCES] Resource.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_UPOSATHA_NAME">
			<Text>Uposatha</Text>
		</Row>
		<Row Tag="LOC_TRADITION_UPOSATHA_DESCRIPTION">
			<Text>+1 [icon:YIELD_HAPPINESS] Happiness Adjacency for [icon:YIELD_HAPPINESS] Happiness Buildings.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_VASSALAGE_NAME">
			<Text>Vassalage</Text>
		</Row>
		<Row Tag="LOC_TRADITION_VASSALAGE_DESCRIPTION">
			<Text>+3 [icon:YIELD_CULTURE] Culture and [icon:YIELD_GOLD] Gold for every [icon:CITYSTATE] City-State you are Suzerain of.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_YEOMANRY_NAME">
			<Text>Yeomanry</Text>
		</Row>
		<Row Tag="LOC_TRADITION_YEOMANRY_DESCRIPTION">
			<Text>+4 [icon:YIELD_FOOD] Food and [icon:YIELD_PRODUCTION] Production in [icon:YIELD_TOWNS] Towns in Distant Lands.</Text>
		</Row>
		<!-- ==================================================	-->
		<!--	CRISIS POLICIES							-->
		<!-- ==================================================	-->
		<Row Tag="LOC_TRADITION_CONSCRIPTION_RIOTS_NAME">
			<Text>Conscription Riots</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CONSCRIPTION_RIOTS_DESCRIPTION">
			<Text>-10 [icon:YIELD_HAPPINESS] Happiness in all [icon:YIELD_CITIES] Cities while at [icon:WAR] War.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_GENDARMERIE_NAME">
			<Text>Gendarmerie</Text>
		</Row>
		<Row Tag="LOC_TRADITION_GENDARMERIE_DESCRIPTION">
			<Text>-4 [icon:YIELD_HAPPINESS] Happiness in each Settlement without a Garrisoned Unit.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_IMPOVERISHED_NOBILITY_NAME">
			<Text>Impoverished Nobility</Text>
		</Row>
		<Row Tag="LOC_TRADITION_IMPOVERISHED_NOBILITY_DESCRIPTION">
			<Text>+1 [icon:YIELD_GOLD] Gold Maintenance for Military Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_NOUVEAU_RICHE_NAME">
			<Text>Nouveau Riche</Text>
		</Row>
		<Row Tag="LOC_TRADITION_NOUVEAU_RICHE_DESCRIPTION">
			<Text>+50% [icon:YIELD_FOOD] Food and [icon:YIELD_HAPPINESS] Happiness Maintenance for [icon:SPECIALIST] Specialists.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_OPPRESSIVE_TARIFFS_NAME">
			<Text>Oppressive Tariffs</Text>
		</Row>
		<Row Tag="LOC_TRADITION_OPPRESSIVE_TARIFFS_DESCRIPTION">
			<Text>-1 [icon:YIELD_GOLD] Gold per [icon:SPECIALIST] Specialist.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_PURGED_OFFICER_CORP_NAME">
			<Text>Purged Officer Corp</Text>
		</Row>
		<Row Tag="LOC_TRADITION_PURGED_OFFICER_CORP_DESCRIPTION">
			<Text>-6 [icon:NAR_REW_COMBAT] Combat Strength on all Military Units.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_REVOLUTIONARY_TRIBUNAL_NAME">
			<Text>Revolutionary Tribunal</Text>
		</Row>
		<Row Tag="LOC_TRADITION_REVOLUTIONARY_TRIBUNAL_DESCRIPTION">
			<Text>-2 [icon:SETTLEMENT_LIMIT] Settlement Limit.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RELIGIOUS_EXEMPTIONS_NAME">
			<Text>Religious Tax Exemptions</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RELIGIOUS_EXEMPTIONS_DESCRIPTION">
			<Text>-2 [icon:YIELD_GOLD] Gold per displayed [icon:NAR_REW_GREATWORK] Relic.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_STRICT_CENSORSHIP_NAME">
			<Text>Strict Censorship</Text>
		</Row>
		<Row Tag="LOC_TRADITION_STRICT_CENSORSHIP_DESCRIPTION">
			<Text>-10 [icon:YIELD_CULTURE] Culture and [icon:YIELD_SCIENCE] Science in [icon:YIELD_CITIES] Cities.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_TAX_FARMING_NAME">
			<Text>Tax Farming</Text>
		</Row>
		<Row Tag="LOC_TRADITION_TAX_FARMING_DESCRIPTION">
			<Text>-50% [icon:YIELD_GOLD] Gold in [icon:YIELD_TOWNS] Towns.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_WEAK_BUREAUCRACY_NAME">
			<Text>Weak Bureaucracy</Text>
		</Row>
		<Row Tag="LOC_TRADITION_WEAK_BUREAUCRACY_DESCRIPTION">
			<Text>-8[icon:YIELD_DIPLOMACY] Influence.</Text>
		</Row>
		<!-- Plague Crisis -->
		<Row Tag="LOC_TRADITION_DIVINE_PUNISHMENT_NAME">
			<Text>Divine Punishment</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DIVINE_PUNISHMENT_DESCRIPTION">
			<Text>+5 [icon:YIELD_HAPPINESS] Happiness in [icon:YIELD_PLAGUE] Infected Settlements that follow your [icon:NAR_REW_RELIGION] Religion. These Settlements gain a [icon:UNIT_MIGRANT] Migrant when [icon:YIELD_PLAGUE] Infected.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DIVINE_MERCY_NAME">
			<Text>Divine Mercy</Text>
		</Row>
		<Row Tag="LOC_TRADITION_DIVINE_MERCY_DESCRIPTION">
			<Text>[icon:CITY_BUILDING_LIST] Buildings, [icon:CITY_RURAL] Improvements, and [icon:CITY_URBAN] Districts in Settlements that follow your [icon:NAR_REW_RELIGION] Religion are not damaged by [icon:YIELD_PLAGUE] Minor Outbreaks.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_HUMORALISM_NAME">
			<Text>Humoralism</Text>
		</Row>
		<Row Tag="LOC_TRADITION_HUMORALISM_DESCRIPTION">
			<Text>Gain 100 [Icon:YIELD_SCIENCE] Science when [icon:UNIT_PHYSICIAN] Physicians Treat the Sick on a [icon:CITY_URBAN] District with a Hospital.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_PLAGUE_CULTS_NAME">
			<Text>Plague Cults</Text>
		</Row>
		<Row Tag="LOC_TRADITION_PLAGUE_CULTS_DESCRIPTION">
			<Text>Gain 100 [Icon:YIELD_CULTURE] Culture when [icon:UNIT_PHYSICIAN] Physicians Treat the Sick on a [icon:CITY_URBAN] District with a Temple.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_LAZARETTOS_NAME">
			<Text>Lazarettos</Text>
		</Row>
		<Row Tag="LOC_TRADITION_LAZARETTOS_DESCRIPTION">
			<Text>[icon:CITY_BUILDING_LIST] Buildings, [icon:CITY_RURAL] Improvements, and [icon:CITY_URBAN] Districts in Coastal Settlements with a Dungeon are not damaged by [icon:YIELD_PLAGUE] Major Outbreaks.</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CORDON_SANITAIRE_NAME">
			<Text>Cordon Sanitaire</Text>
		</Row>
		<Row Tag="LOC_TRADITION_CORDON_SANITAIRE_DESCRIPTION">
			<Text>[icon:CITY_BUILDING_LIST] Buildings, [icon:CITY_RURAL] Improvements and [icon:CITY_URBAN] Districts in Settlements with a stationed Commander are not damaged by [icon:YIELD_PLAGUE] Major Outbreaks.</Text>
		</Row>
		<!-- Religion Crisis -->
		<Row Tag="LOC_TRADITION_RELIGIOUS_TOLERANCE_NAME">
			<Text>Religious Tolerance</Text>
		</Row>
		<Row Tag="LOC_TRADITION_RELIGIOUS_TOLERANCE_DESCRIPTION">
