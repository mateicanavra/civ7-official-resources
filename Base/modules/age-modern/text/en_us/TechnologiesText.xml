<?xml version="1.0" encoding="utf-8"?>
<Database>
	<EnglishText>
		<!-- Names -->
		<Row Tag="LOC_TREE_TECHS_MO_NAME">
			<Text>Modern Technology</Text>
		</Row>
		<Row Tag="LOC_TECH_ACADEMICS_NAME">
			<Text>Academics</Text>
		</Row>
		<Row Tag="LOC_TECH_ACADEMICS_QUOTE">
			<Text>"Now is the time to understand more, so that we may fear less."</Text>
		</Row>
		<Row Tag="LOC_TECH_ACADEMICS_QUOTE_AUTHOR">
			<Text>Marie Curie</Text>
		</Row>
		<Row Tag="LOC_TECH_STEAM_ENGINE_NAME">
			<Text>Steam Engine</Text>
		</Row>
		<Row Tag="LOC_TECH_STEAM_ENGINE_QUOTE">
			<Text>"Soon shall thy arm, unconquered steam, afar drag the slow barge or drive the rapid car."</Text>
		</Row>
		<Row Tag="LOC_TECH_STEAM_ENGINE_QUOTE_AUTHOR">
			<Text>Erasmus Darwin</Text>
		</Row>
		<Row Tag="LOC_TECH_MILITARY_SCIENCE_NAME">
			<Text>Military Science</Text>
		</Row>
		<Row Tag="LOC_TECH_MILITARY_SCIENCE_QUOTE">
			<Text>"Confuse your enemy. Keep him in the dark about your intentions."</Text>
		</Row>
		<Row Tag="LOC_TECH_MILITARY_SCIENCE_QUOTE_AUTHOR">
			<Text>Võ Nguyên Giáp</Text>
		</Row>
		<Row Tag="LOC_TECH_URBANIZATION_NAME">
			<Text>Urbanization</Text>
		</Row>
		<Row Tag="LOC_TECH_URBANIZATION_QUOTE">
			<Text>"The face of a city changes faster than a human heart."</Text>
		</Row>
		<Row Tag="LOC_TECH_URBANIZATION_QUOTE_AUTHOR">
			<Text>Charles Baudelaire</Text>
		</Row>
		<Row Tag="LOC_TECH_MASS_PRODUCTION_NAME">
			<Text>Mass Production</Text>
		</Row>
		<Row Tag="LOC_TECH_MASS_PRODUCTION_QUOTE">
			<Text>"Just as modern mass production requires the standardization of commodities, so the social process requires standardization of man."</Text>
		</Row>
		<Row Tag="LOC_TECH_MASS_PRODUCTION_QUOTE_AUTHOR">
			<Text>Erich Fromm</Text>
		</Row>
		<Row Tag="LOC_TECH_ELECTRICITY_NAME">
			<Text>Electricity</Text>
		</Row>
		<Row Tag="LOC_TECH_ELECTRICITY_QUOTE">
			<Text>"The beauty of electricity is not that the power is mysterious, but that it is under law, and that the taught intellect can even now govern it."</Text>
		</Row>
		<Row Tag="LOC_TECH_ELECTRICITY_QUOTE_AUTHOR">
			<Text>Michael Faraday</Text>
		</Row>
		<Row Tag="LOC_TECH_INDUSTRIALIZATION_NAME">
			<Text>Industrialization</Text>
		</Row>
		<Row Tag="LOC_TECH_INDUSTRIALIZATION_QUOTE">
			<Text>"And was Jerusalem builded here among these dark Satanic Mills?"</Text>
		</Row>
		<Row Tag="LOC_TECH_INDUSTRIALIZATION_QUOTE_AUTHOR">
			<Text>William Blake</Text>
		</Row>
		<Row Tag="LOC_TECH_RADIO_NAME">
			<Text>Radio</Text>
		</Row>
		<Row Tag="LOC_TECH_RADIO_QUOTE">
			<Text>"We annihilated the world before your very ears… You will be relieved, I hope, to learn that we didn't mean it."</Text>
		</Row>
		<Row Tag="LOC_TECH_RADIO_QUOTE_AUTHOR">
			<Text>Orson Welles</Text>
		</Row>
		<Row Tag="LOC_TECH_FLIGHT_NAME">
			<Text>Flight</Text>
		</Row>
		<Row Tag="LOC_TECH_FLIGHT_QUOTE">
			<Text>"The sound of wings."</Text>
		</Row>
		<Row Tag="LOC_TECH_FLIGHT_QUOTE_AUTHOR">
			<Text>Amelia Earhart</Text>
		</Row>
		<Row Tag="LOC_TECH_COMBUSTION_NAME">
			<Text>Combustion</Text>
		</Row>
		<Row Tag="LOC_TECH_COMBUSTION_QUOTE">
			<Text>"So readily do people adapt themselves to the marvelous contrivances which modern inventive ingenuity has devised."</Text>
		</Row>
		<Row Tag="LOC_TECH_COMBUSTION_QUOTE_AUTHOR">
			<Text>The Marvelous Vogue of the Automobile</Text>
		</Row>
		<Row Tag="LOC_TECH_MOBILIZATION_NAME">
			<Text>Mobilization</Text>
		</Row>
		<Row Tag="LOC_TECH_MOBILIZATION_QUOTE">
			<Text>"The young men shall fight; the women shall make tents; the old shall arouse the courage of the warriors."</Text>
		</Row>
		<Row Tag="LOC_TECH_MOBILIZATION_QUOTE_AUTHOR">
			<Text>Levée en Masse</Text>
		</Row>
		<Row Tag="LOC_TECH_ARMOR_NAME">
			<Text>Armor</Text>
		</Row>
		<Row Tag="LOC_TECH_ARMOR_QUOTE">
			<Text>"Bombardment, barrage, curtain-fire, mines, gas, tanks, machine-guns, hand-grenades - words, words, but they hold the horror of the world."</Text>
		</Row>
		<Row Tag="LOC_TECH_ARMOR_QUOTE_AUTHOR">
			<Text>Erich Maria Remarque</Text>
		</Row>
		<Row Tag="LOC_TECH_AERODYNAMICS_NAME">
			<Text>Aerodynamics</Text>
		</Row>
		<Row Tag="LOC_TECH_AERODYNAMICS_QUOTE">
			<Text>"All birds need to fly are the right-shaped wings, the right pressure, and the right angle."</Text>
		</Row>
		<Row Tag="LOC_TECH_AERODYNAMICS_QUOTE_AUTHOR">
			<Text>Daniel Bernoulli</Text>
		</Row>
		<Row Tag="LOC_TECH_ROCKETRY_NAME">
			<Text>Rocketry</Text>
		</Row>
		<Row Tag="LOC_TECH_ROCKETRY_QUOTE">
			<Text>"A screaming comes across the sky."</Text>
		</Row>
		<Row Tag="LOC_TECH_ROCKETRY_QUOTE_AUTHOR">
			<Text>Thomas Pynchon</Text>
		</Row>
		<Row Tag="LOC_TECH_NUCLEAR_FISSION_NAME">
			<Text>Nuclear Fission</Text>
		</Row>
		<Row Tag="LOC_TECH_NUCLEAR_FISSION_QUOTE">
			<Text>"Scientists are not dependent on the ideas of a single man, but on the combined wisdom of thousands."</Text>
		</Row>
		<Row Tag="LOC_TECH_NUCLEAR_FISSION_QUOTE_AUTHOR">
			<Text>Ernest Rutherford</Text>
		</Row>
		<Row Tag="LOC_TECH_FUTURE_TECH_NAME">
			<Text>Future Tech</Text>
		</Row>
		<Row Tag="LOC_TECH_FUTURE_TECH_QUOTE">
			<Text></Text>
		</Row>
		<Row Tag="LOC_TECH_FUTURE_TECH_AUTHOR">
			<Text></Text>
		</Row>
		<Row Tag="LOC_MOD_MO_FUTURE_TECH_AGE_PROGRESS_DESCRIPTION">
			<Text>+10 Age Progress.</Text>
		</Row>
		<!-- Tech Standard Modifiers -->
		<Row Tag="LOC_MOD_TECH_MO_EMBARKATION_DESCRIPTION">
			<Text>Trade Units can enter deep Ocean Terrain. Settlements across deep Ocean can now serve as origin Settlements for [icon:TRADE_ROUTE] Trade Routes.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_SETTLEMENT_CAP_INCREASE_DESCRIPTION">
			<Text>+1 [icon:SETTLEMENT_LIMIT] Settlement Limit.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_SPECIALIST_CAP_INCREASE_DESCRIPTION">
			<Text>+1 [icon:SPECIALIST] Specialist Limit in all [icon:YIELD_CITIES] Cities.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_CAVALRY_STRENGTH_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for Cavalry Units.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_INFANTRY_STRENGTH_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for Infantry Units.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_INTERCEPT_RANGE_DESCRIPTION">
			<Text>+1 Intercept Range for Fighter Units.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_NAVAL_STRENGTH_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for Naval Units.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_RANGED_STRENGTH_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for Ranged and Siege Units.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_AIRCRAFT_STRENGTH_DESCRIPTION">
			<Text>+3 [icon:NAR_REW_COMBAT] Combat Strength for Air Units.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_CULTURE_BUILDING_DESCRIPTION">
			<Text>+1 [icon:YIELD_CULTURE] Culture on [icon:YIELD_CULTURE] Culture Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_FOOD_BUILDING_DESCRIPTION">
			<Text>+1 [icon:YIELD_FOOD] Food on [icon:YIELD_FOOD] Food Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_GOLD_BUILDING_DESCRIPTION">
			<Text>+1 [icon:YIELD_GOLD] Gold on [icon:YIELD_GOLD] Gold Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_HAPPINESS_BUILDING_DESCRIPTION">
			<Text>+1 [icon:YIELD_HAPPINESS] Happiness on [icon:YIELD_HAPPINESS] Happiness Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_MILITARY_BUILDING_DESCRIPTION">
			<Text>+2 [icon:YIELD_PRODUCTION] Production on [icon:MILITARY] Military Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_PRODUCTION_BUILDING_DESCRIPTION">
			<Text>+1 [icon:YIELD_PRODUCTION] Production on [icon:YIELD_PRODUCTION] Production Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_TECH_SCIENCE_BUILDING_DESCRIPTION">
			<Text>+1 [icon:YIELD_SCIENCE] Science on [icon:YIELD_SCIENCE] Science Buildings.</Text>
		</Row>
		<Row Tag="LOC_MOD_MO_FUTURE_TECH_ATTRIBUTE_DESCRIPTION">
			<Text>+1 [icon:ATTRIBUTE] Random Attribute Point.</Text>
		</Row>
		<!-- Tech Improvement Modifiers -->
		<Row Tag="LOC_MOD_TECH_MO_MINE_PRODUCTION_DESCRIPTION">
			<Text>+1[icon:YIELD_PRODUCTION] Production on Mines and Woodcutters.</Text>
		</Row>
		<Row Tag="LOC_MOD_TECH_MO_OIL_PRODUCTION_DESCRIPTION">
			<Text>+1[icon:YIELD_PRODUCTION] Production on Oil Rigs.</Text>
		</Row>
		<Row Tag="LOC_MOD_TECH_MO_FISHING_BOAT_RESOURCE_FOOD_DESCRIPTION">
			<Text>+1[icon:YIELD_FOOD] Food on Fishing Boats and Pastures.</Text>
		</Row>
		<Row Tag="LOC_MOD_TECH_MO_QUARRY_PRODUCTION_DESCRIPTION">
			<Text>+1[icon:YIELD_PRODUCTION] Production on Clay Pits and Quarries.</Text>
		</Row>
		<Row Tag="LOC_MOD_TECH_MO_PLANTATION_FOOD_DESCRIPTION">
			<Text>+1[icon:YIELD_FOOD] Food on Farms and Plantations.</Text>
		</Row>
		<Row Tag="LOC_MOD_TECH_MO_CAMP_PRODUCTION_DESCRIPTION">
			<Text>+1[icon:YIELD_PRODUCTION] Production on Camps.</Text>
		</Row>
	</EnglishText>
</Database>
